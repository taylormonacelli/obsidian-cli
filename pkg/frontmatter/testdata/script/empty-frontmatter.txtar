# Test file with empty frontmatter (just delimiters)
cp empty-fm.md test-vault/empty-fm.md
cp empty-fm.md test-vault/empty-fm.md.backup

exec obsidian-cli frontmatter print .title empty-fm.md
cmp stdout empty.golden
! stderr .
cmp test-vault/empty-fm.md test-vault/empty-fm.md.backup

# Test adding field to empty frontmatter
cp empty-fm.md test-vault/temp.md
exec obsidian-cli frontmatter edit '.title = "New Title"' temp.md
! stderr .
cmp test-vault/temp.md with-title.golden

-- empty-fm.md --
